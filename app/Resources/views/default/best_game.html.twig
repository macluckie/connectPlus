<section class="featured-places" id="blog">
    <div class="container">
  <div class="row">
      <div class="col-md-12">
          <div class="section-heading">
              <h2>Services</h2>
          </div>
      </div> 
  </div> 
  <div class="row">
        {% for service in services %}
      <div class="col-md-4 col-sm-6 col-xs-12">
          <div class="featured-item">
              <div class="thumb">
                  {% if service.imageName is null %}
                  <img loading="lazy" src="template/img/featured_item_1.jpg" alt="">
                  {% else %}
                  <img src="{{asset('upload/resize'~service.imageName)}}" alt="">
             
                  {% endif %}
              </div>
              <div class="down-content">
                  <h4>{{service.title}}</h4>                  
                  <p>{{service.details}}<br>{{service.details2}}</p>
                  <div class="row">                   
                      <div class="col-md-6">
                          <div class="text-button">
                              <a href="#">Continue Reading</a>
                          </div>
                      </div>
                  </div>
              </div>
          </div>
      </div>
        {% endfor %}
